{
  "name": "@campuscalendar/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run-many --target=serve --projects=api-gateway,auth-service,school-service,user-service --parallel --maxParallel=4",
    "docker:dev": "docker-compose -f docker/dev/docker-compose.yml -f docker/dev/docker-compose.school-service.yml -f docker/dev/docker-compose.user-service.yml -f docker/dev/docker-compose.flyway.yml --env-file docker/.docker-compose.env up -d --build",
    "docker:prod": "npm run build:all && docker-compose -f docker/prod/docker-compose.yml -f docker/prod/docker-compose.school-service.yml -f docker/prod/docker-compose.user-service.yml -f docker/prod/docker-compose.auth-service.yml -f docker/prod/docker-compose.flyway.yml --env-file docker/.docker-compose.env up -d --build",
    "migration:generate": "npx mikro-orm migration:create --initial",
    "build:all": "nx run-many --targets=build -p api-gateway,auth-service,school-service,user-service,frontend --parallel --maxParallel=4",
    "build:docker": "docker build -t campuscalendar/api-gateway:latest -f apps/api-gateway/docker/Dockerfile.prod . && docker build -t campuscalendar/auth-service:latest -f apps/auth-service/docker/Dockerfile.prod . && docker build -t campuscalendar/school-service:latest -f apps/school-service/docker/Dockerfile.prod . && docker build -t campuscalendar/user-service:latest -f apps/user-service/docker/Dockerfile.prod .",
    "push:docker": "docker push campuscalendar/api-gateway:latest && docker push campuscalendar/auth-service:latest && docker push campuscalendar/school-service:latest && docker push campuscalendar/user-service:latest"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "16.2.1",
    "@angular/common": "16.2.1",
    "@angular/compiler": "16.2.1",
    "@angular/core": "16.2.1",
    "@angular/forms": "16.2.1",
    "@angular/platform-browser": "16.2.1",
    "@angular/platform-browser-dynamic": "16.2.1",
    "@angular/router": "16.2.1",
    "@mikro-orm/core": "^5.7.12",
    "@mikro-orm/mysql": "^5.7.12",
    "@mikro-orm/nestjs": "^5.1.8",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^10.0.0",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nx/angular": "17.0.3",
    "amqp-connection-manager": "^4.1.13",
    "amqplib": "^0.10.3",
    "argon2": "^0.30.3",
    "axios": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.0",
    "@angular-devkit/core": "16.2.0",
    "@angular-devkit/schematics": "16.2.0",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.0.0",
    "@angular/compiler-cli": "16.2.1",
    "@angular/language-service": "16.2.1",
    "@mikro-orm/cli": "^5.7.12",
    "@mikro-orm/migrations": "^5.7.12",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.1.3",
    "@nx/cypress": "17.0.3",
    "@nx/eslint-plugin": "17.0.3",
    "@nx/jest": "17.0.3",
    "@nx/js": "17.0.3",
    "@nx/linter": "17.0.3",
    "@nx/nest": "17.0.3",
    "@nx/node": "17.0.3",
    "@nx/web": "17.0.3",
    "@nx/webpack": "17.0.3",
    "@nx/workspace": "17.0.3",
    "@schematics/angular": "16.2.0",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/jest": "^29.4.0",
    "@types/node": "~18.7.1",
    "@types/uuid": "^9.0.2",
